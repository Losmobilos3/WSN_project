   "source": [
    "l = 0.125\n",
    "\n",
    "dBm_to_mW = lambda p_dBm: 10**(p_dBm / 10)\n",
    "\n",
    "p_tx_dBm = -24 # dBm\n",
    "p_tx = dBm_to_mW(p_tx_dBm) # mW\n",
    "p_rx_dBm = -90 # dBm\n",
    "p_rx = dBm_to_mW(p_rx_dBm) # mW\n",
    "\n",
    "d = np.sqrt((p_tx * 1 * 1 * (l)**2)/((4*np.pi)**2 * p_rx * 1))\n",
    "d.item()"
